<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healthsolutionsapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HealthSolutionsApplication">

        <activity
            android:name=".Activity.ChangePasswordActivity"
            android:parentActivityName=".Activity.PersonalActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.PersonalActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.EmailActivity"
            android:exported="true"
            android:parentActivityName=".Activity.ChangeInformationActivity" />
        <activity
            android:name=".Activity.PhoneNumberActivity"
            android:exported="true"
            android:parentActivityName=".Activity.ChangeInformationActivity" />
        <activity
            android:name=".Activity.FullNameActivity"
            android:exported="true"
            android:parentActivityName=".Activity.ChangeInformationActivity" />
        <activity
            android:name=".Activity.GenderActivity"
            android:exported="true"
            android:parentActivityName=".Activity.ChangeInformationActivity" />
        <activity
            android:name=".Activity.ChangeInformationActivity"
            android:parentActivityName=".Activity.PersonalActivity"
            android:exported="true"/>
        <activity
            android:name=".Activity.DobActivity"
            android:exported="true"
            android:parentActivityName=".Activity.ChangeInformationActivity" />
        <activity
            android:name=".Activity.MainActivity"
            android:exported="true" />
    </application>

</manifest>